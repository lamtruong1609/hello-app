{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "properties": {
        "autoscaling": {
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "maxReplicas": {
                    "type": "integer"
                },
                "minReplicas": {
                    "type": "integer"
                },
                "targetCPUUtilizationPercentage": {
                    "type": "integer"
                }
            },
            "type": "object"
        },
        "fullnameOverride": {
            "type": "string"
        },
        "image": {
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "imagePullSecrets": {
            "type": "array"
        },
        "livenessProbe": {
            "properties": {
                "httpGet": {
                    "properties": {
                        "path": {
                            "type": "string"
                        },
                        "port": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                }
            },
            "type": "object"
        },
        "nameOverride": {
            "type": "string"
        },
        "podAnnotations": {
            "properties": {},
            "type": "object"
        },
        "podLabels": {
            "properties": {},
            "type": "object"
        },
        "podSecurityContext": {
            "properties": {
                "fsGroup": {
                    "type": "integer"
                }
            },
            "type": "object"
        },
        "readinessProbe": {
            "properties": {
                "httpGet": {
                    "properties": {
                        "path": {
                            "type": "string"
                        },
                        "port": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                }
            },
            "type": "object"
        },
        "replicaCount": {
            "type": "integer"
        },
        "resources": {
            "properties": {},
            "type": "object"
        },
        "securityContext": {
            "properties": {
                "capabilities": {
                    "properties": {
                        "drop": {
                            "items": {
                                "type": "string"
                            },
                            "type": "array"
                        }
                    },
                    "type": "object"
                },
                "readOnlyRootFilesystem": {
                    "type": "boolean"
                },
                "runAsNonRoot": {
                    "type": "boolean"
                },
                "runAsUser": {
                    "type": "integer"
                }
            },
            "type": "object"
        },
        "service": {
            "properties": {
                "port": {
                    "type": "integer"
                },
                "type": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "volumeMounts": {
            "type": "array"
        },
        "volumes": {
            "type": "array"
        }
    },
    "type": "object"
}
